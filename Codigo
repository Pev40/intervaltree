
#include <iostream>
using namespace std;

class Interval
{
    public:
        int low, high;      
}

class Node
{   
    private:
        Interval *i;  
        int max;
        Nodo *left, *right;
        
    public:
        void newNode(Interval i){
            Node *temp = new Node;
            temp->i = new Interval(i);
            temp->max = i.high;
            temp->left = temp->right = NULL;
            return temp;
        }
        
        void InsertNode();
        void EliminarNode();
        void BuscarNode();
}



