
#include <iostream>
using namespace std;

class Interval
{
    public:
        int low, high;
        Interval(int low , int high)
        {
            this->low = low;
            this->high = high;
        }
       
}

class Node
{   
    private:
        Interval *i;  
        int max;
        Nodo *left, *right;
        
    public:
        void newNode(Interval i){
            Node *temp = new Node;
            temp->i = new Interval(i);
            temp->max = i.high;
            temp->left = temp->right = NULL;
            return temp;
        }
        
        void InsertNode(Node *head , Interval i )
        {   
            if (head == NULL)
                return newNode(i);
            
        }
        void EliminarNode();
        void BuscarNode();
}



